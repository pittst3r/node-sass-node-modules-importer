{
  "name": "node-sass-node-modules-importer",
  "version": "0.1.0",
  "description": "A node-sass custom importer which turns ~ into absolute paths to the nearest parent node_modules directory.",
  "repository": "https://github.com/pittst3r/node-sass-node-modules-importer",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "node-sass",
    "scss",
    "sass",
    "tilde",
    "webpack",
    "sass-loader",
    "node_modules"
  ],
  "contributors": [
    "Matthew Davidson <matthew.davidson@skyscanner.net>",
    "Robbie Pitts <me@robbie-pitts.com>"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "find-parent-dir": "^0.3.0"
  },
  "devDependencies": {
    "jest": "^24.1.0"
  },
  "jest": {
    "verbose": true,
    "testRegex": ".+\\.test.js$",
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "statements": 75,
        "branches": 75,
        "functions": 75,
        "lines": 75
      }
    }
  }
}
